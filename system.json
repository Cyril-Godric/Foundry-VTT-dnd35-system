    },
    {
      "name": "items",
      "label": "Items (SRD)",
      "system": "dnd3.5e",
      "path": "./packs/items.db",
      "entity": "Item"
    },
    {
      "name": "spells",
      "label": "Spells (SRD)",
      "system": "dnd3.5e",
      "path": "./packs/spells.db",
      "entity": "Item"
    },
    {
      "name": "classes",
      "label": "Classes (SRD)",
      "system": "dnd3.5e",
      "path": "./packs/classes.db",
      "entity": "Item"
    },
    {
      "name": "classfeatures",
      "label": "Class Features (SRD)",
      "system": "dnd3.5e",
      "path": "./packs/classfeatures.db",
      "entity": "Item"
    }
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
